generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
    provider = "mysql"
    url      = env("DB_URL")
}

model User {
    id           String @id
    username     String
    age          Int
    bio          String
    profileEmoji String

    @@map("users")
}
